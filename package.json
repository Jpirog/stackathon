{
  "name": "stackathon",
  "version": "1.0.0",
  "description": "Stackathon project - John Pirog - Aug 2021",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seedold": "node script/seed.js",
    "seed": "SEED=true node script/seed.js",
    "start": "node -r esm server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "nodemon -r esm server -e html,js,scss --ignore public --ignore client",
    "start-server-old": "JWT=shh nodemon -r esm server -e html,js,scss --ignore public --ignore client",
    "testold": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "John Pirog",
  "license": "ISC",
  "dependencies": {
    "@emotion/styled": "^11.3.0",
    "@vonage/server-sdk": "^2.10.10",
    "axios": "^0.21.1",
    "dateformat": "^4.5.1",
    "deepai": "^1.0.17",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "react-hot-toast": "^2.1.1",
    "react-number-format": "^4.7.3",
    "react-paginate": "^7.1.3",
    "redux-devtools-extension": "^2.13.9",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3",
    "twitter-api-v2": "^1.3.0",
    "ws": "^7.4.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
